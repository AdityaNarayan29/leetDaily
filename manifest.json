{
  "manifest_version": 3,
  "name": "LeetDaily",
  "version": "1.0",
  "description": "One-click access to LeetCode's daily question.",
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "permissions": ["tabs", "storage"],
  "host_permissions": ["https://leetcode.com/*"]
}
