{
  "name": "leetdaily",
  "displayName": "LeetDaily",
  "version": "2.0.0",
  "description": "Chrome extension for LeetCode daily challenges, Blind 75/NeetCode 150/LC 75 tracking, FAANG company tags, 2000+ problems explorer, streak milestones, 30-day heatmap & smart reminders.",
  "main": "popup.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:css": "tailwindcss -i ./styles/input.css -o ./styles/output.css --minify",
    "dev:css": "tailwindcss -i ./styles/input.css -o ./styles/output.css -w",
    "build": "npm run build:css",
    "fetch-problems": "node scripts/fetch-problems.js",
    "fetch-problems:full": "node scripts/fetch-problems.js --with-companies"
  },
  "keywords": [
    "leetcode",
    "chrome-extension",
    "daily-challenge",
    "coding-practice",
    "productivity",
    "streak-tracker",
    "blind-75",
    "neetcode-150",
    "problems-explorer",
    "company-tags",
    "coding-interview"
  ],
  "author": "Aditya Narayan <masst.dev@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/adityanarayan/leetdaily"
  },
  "bugs": {
    "url": "https://github.com/adityanarayan/leetdaily/issues"
  },
  "homepage": "https://github.com/adityanarayan/leetdaily#readme",
  "dependencies": {
    "@tailwindcss/cli": "^4.1.18",
    "tailwindcss": "^4.1.18"
  }
}
